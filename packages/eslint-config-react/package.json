{
  "name": "@pacner/eslint-config-react",
  "version": "0.2.0",
  "description": "ESLint shareable config with vitamins for React development",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "keywords": [
    "pacner",
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "react"
  ],
  "author": {
    "name": "JosÃ© Manuel Rosa Moncayo",
    "email": "josemanuel.rosamoncayo@gmail.com"
  },
  "homepage": "https://github.com/jmanuelrosa/pacner/packages/eslint-config-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmanuelrosa/pacner.git"
  },
  "bugs": {
    "url": "https://github.com/jmanuelrosa/pacner/issues?q=is%3Aissue+label%eslint-config-react"
  },
  "engines": {
    "node": ">= 14.17.3",
    "npm": ">= 7.20.3",
    "yarn": ">=1.22.10"
  },
  "files": [
    "index.js",
    "javascript.js",
    "ts.js",
    "rules"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.json,.md",
    "fix": "eslint . --fix --ext .js,.json,.md",
    "rules:list": "eslint --config .eslintrc --print-config",
    "rules:new": "eslint-find-rules --unused index.js",
    "cleanup": "yarn cache clean --force"
  },
  "dependencies": {
    "@pacner/eslint-config": "^0.1.0",
    "eslint": "latest"
  },
  "devDependencies": {
    "eslint-config-standard-jsx": "latest",
    "eslint-config-standard-react": "latest",
    "eslint-find-rules": "latest",
    "eslint-plugin-cypress": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-unicorn": "latest"
  },
  "peerDependencies": {
    "@pacner/eslint-config": "0.0.1",
    "eslint": "latest",
    "eslint-config-standard-jsx": "latest",
    "eslint-config-standard-react": "latest",
    "eslint-plugin-cypress": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-unicorn": "latest"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-cypress": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
