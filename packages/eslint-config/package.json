{
  "name": "@pacner/eslint-config",
  "version": "0.1.0",
  "description": "ESLint shareable config with vitamins",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "keywords": [
    "pacner",
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide"
  ],
  "author": {
    "name": "JosÃ© Manuel Rosa Moncayo",
    "email": "josemanuel.rosamoncayo@gmail.com"
  },
  "homepage": "https://github.com/jmanuelrosa/pacner/packages/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmanuelrosa/pacner.git"
  },
  "bugs": {
    "url": "https://github.com/jmanuelrosa/pacner/issues?q=is%3Aissue+label%eslint-config"
  },
  "engines": {
    "node": ">= 14.17.3",
    "npm": ">= 7.20.3",
    "yarn": ">=1.22.10"
  },
  "files": [
    "index.js",
    "javascript.js",
    "ts.js",
    "rules"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.json,.md",
    "fix": "eslint . --fix --ext .js,.json,.md",
    "rules:list": "eslint --config .eslintrc --print-config",
    "rules:new": "eslint-find-rules --unused index.js",
    "cleanup": "yarn cache clean --force"
  },
  "dependencies": {
    "eslint": "latest"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint-config-standard": "latest",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-find-rules": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsonc": "latest",
    "eslint-plugin-markdown": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-unicorn": "latest",
    "typescript": "latest"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsonc": "latest",
    "eslint-plugin-markdown": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-unicorn": "latest",
    "typescript": "latest"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-config-standard-with-typescript": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jsonc": {
      "optional": true
    },
    "eslint-plugin-markdown": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
